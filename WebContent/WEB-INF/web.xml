<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>com.giorgiogamberini.parallelism.webclient</display-name>
  <welcome-file-list>
    <welcome-file>index.xhtml</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>Message Driven Process Spinner</servlet-name>
    <servlet-class>javax.faces.webapp.FacesServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>Message Driven Process Spinner</servlet-name>
    <url-pattern>/mdps/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <display-name>HellsWorlds</display-name>
    <servlet-name>HellsWorlds</servlet-name>
    <servlet-class>com.giorgiogamberini.parallelism.webclient.ws.HellsWorld</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HellsWorlds</servlet-name>
    <url-pattern>/HellsWorlds</url-pattern>
  </servlet-mapping>
</web-app>